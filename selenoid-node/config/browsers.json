{
    {{#each cfg.settings.browsers as |browser browser_settings| ~}}
    {{#if browser_settings.windows.versions }}
    {{#unless @first}},{{/unless}}"{{ browser_settings.name }}": {
        "default": "{{browser_settings.windows.versions[0].browser}}",
        "versions": {
            {{#each browser_settings.windows.versions as |version| ~}}
            {{#unless @first}},{{/unless}}"{{version.browser}}": {
                "image": ["pwsh", "C:/hab/svc/selenoid-node/config/start_{{browser}}.ps1", "-browser", "{{version.browser}}", "-driver", "{{version.driver}}"]
            }
            {{/each ~}}
        }
    }
    {{/if}}
    {{#if browser_settings.docker.versions }}
    {{#unless @first}},{{/unless}}"{{ browser_settings.name }}": {
        "default": "{{browser_settings.docker.versions[0].browser}}",
        "versions": {
            {{#each browser_settings.docker.versions as |version| ~}}
            {{#unless @first}},{{/unless}}"{{version.browser}}": {
                "image": "selenoid/vnc:{{ browser }}_{{version.browser}}",
                "port": "4444",
                "path": "{{browser_settings.path}}",
                "tmpfs": {
                    "/tmp": "size=128m"
                }
            }
            {{/each ~}}
        }
    }
    {{/if}}
    {{/each ~}}
}
